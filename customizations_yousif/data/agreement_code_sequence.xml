<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <!-- Sequences for agreement code -->
        <record id="seq_agreement_code" model="ir.sequence">
            <field name="name">Agreement Code</field>
            <field name="code">project.agreement.code.sequence</field>
            <field name="prefix">PA-%(month)s-%(y)s-</field>
            <field name="padding">4</field>
            <field name="number_increment">1</field>
            <field name="implementation">no_gap</field>
            <field name="active" eval="True"/>
            <field name="company_id" eval="False"/>
        </record>
    </data>
</odoo>
